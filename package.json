{
  "name": "handy-links",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000 --host",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "npx eslint ./src --fix --ext .vue --ext .ts --ext .vue",
    "prepare": "husky",
    "deploy": "surge --project ./dist --domain handy-links.surge.sh"
  },
  "dependencies": {
    "vue": "^3.5.12",
    "vuetify": "^3.7.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-vue": "^5.1.4",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.26.0",
    "husky": "^9.1.6",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.6"
  }
}
